# This file includes environment variables that can be committed to git. It's gitignored by default because it represents your local development environment.

# Built-in environment variables
APP_DISPLAY_NAME=Insurance Agent
APP_INTERNAL_NAME=insurance-agent
TEAMSFX_ENV=local
APP_NAME_SUFFIX=local

# AI Models Configuration
MODELS_ENDPOINT=REPLACE_WITH_AI_FOUNDRY_ENDPOINT_URL

# Main language model gpt-4.1 for general language understanding
LANGUAGE_MODEL_NAME=gpt-4.1

# Vision & Fraud analysis model (mistral-medium-2505)
VISION_MODEL_NAME=mistral-medium-2505

# Embedding model for semantic search
EMBEDDING_MODEL_NAME=text-embedding-ada-002

# Azure AI Search
AZURE_AI_SEARCH_ENDPOINT=REPLACE_WITH_YOUR_AI_SEARCH_ENDPOINT
AZURE_AI_SEARCH_API_KEY=REPLACE_WITH_YOUR_AI_SEARCH_API_KEY

# Storage settings
AZURE_STORAGE_CONNECTION_STRING=UseDevelopmentStorage=true
AZURE_STORAGE_ACCOUNT_NAME=REPLACE_WITH_YOUR_STORAGE_ACCOUNT_NAME

# Blob Storage for Damage Photos
BLOB_STORAGE_CONTAINER_NAME=claim-photos
BLOB_STORAGE_BASE_URL=https://REPLACE_WITH_YOUR_STORAGE_ACCOUNT_NAME.blob.core.windows.net

# Generated during provision
BOT_SERVICE_PRINCIPAL_ID=_

