{
  "name": "zava-claims-ops",
  "version": "1.0.0",
  "description": "Home insurance claims operations service - provides AI agents with access to claims, contractors, and inspection data",
  "main": "dist/src/functions/*.js",
  "bin": {
    "zava-claims-mcp-server": "dist/src/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "start": "func start",
    "start:azurite": "azurite --silent --location azurite --debug azurite/debug.log",
    "start:devtunnel": "node ../zava-deployment-scripts/run-devtunnel.js --id zava-mcp --port 3001 --allow-anonymous",
    "init-data": "npm run build && node data/generate-data.js && node dist/src/scripts/initializeData.js",
    "init-data-prod": "npm run build && node dist/src/scripts/initializeAzureData.js",
    "start:mcp-http": "npm run build && node dist/src/mcp-server-http.js",
    "inspector": "npx @modelcontextprotocol/inspector --transport http --server-url http://localhost:3001/mcp/messages",
    "inspector-prod": "npx @modelcontextprotocol/inspector --transport http --server-url https://claims-mcp-app.proudglacier-5e06022c.eastus.azurecontainerapps.io/mcp/messages",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "insurance",
    "claims",
    "mcp-server",
    "model-context-protocol",
    "typescript",
    "home-insurance",
    "ai-agents",
    "zava"
  ],
  "author": "Microsoft 365 Cloud Advocates",
  "license": "MIT",
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/functions": "^4.0.0",
    "@azure/msal-node": "^3.8.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-validate": "^0.5.1",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "sharp": "^0.32.4",
    "uuid": "^9.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.17.1",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.4.5",
    "@types/passport": "^1.0.17",
    "@types/passport-oauth2": "^1.8.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "azure-functions-core-tools": "^4.0.5095",
    "azurite": "^3.28.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
